fun csv_parse(file_path) {
	raw_lines = split(read(file_path), "\n");
	return_list = [];
	
	str_buff = "";
	last_char = "";
	is_str = false;
	for line in raw_lines {
		#before line
		row = [];
		if NE(trim(line), "") {
			for character in line {
				if and(E(character, "\""), N(is_str)) {
					is_str = true;
				}
				elif and(E(character, "\""), E(last_char, "\"")) {
					#in quote quote
					push(str_buff, "\"");
					last_char = "";
				}
				elif and(E(character, "\""), is_str) {
					is_str = false;
					push(row, str_buff);
					str_buff = "";
					last_char = character;
				}
				elif is_str {
					push(str_buff, character);
					last_char = character;
				}
				elif E(character, ",") {
					push(row, str_buff);
				}
				else {
					push(str_buff, character);
				}
			}
		}
		push(return_list, row);
		#end of line
	}
	return return_list;
}

parsed_csv = csv_parse("addresses.csv");

for row in parsed_csv {
	for column in row {
		print(cat("//", column, "//"));
	}
	print("\n");
}